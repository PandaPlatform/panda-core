{
    "name": "panda/framework",
    "description": "The Panda PHP Framework.",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "homepage": "http://pandaphp.org",
    "support": {
        "source": "https://github.com/PandaPlatform/panda-framework",
        "issues": "https://github.com/PandaPlatform/panda-framework/issues"
    },
    "authors": [
        {
            "name": "Ioannis Papikas",
            "email": "papikas.ioan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "php-di/php-di": "5.4.*",
        "psr/http-message": "~1.0",
        "monolog/monolog": "~1.22",
        "symfony/http-foundation": "3.2.*",
        "symfony/http-kernel": "3.2.*",
        "symfony/routing": "3.2.*"
    },
    "replace": {
        "panda/contracts": "self.version",
        "panda/helpers": "self.version",
        "panda/storage": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Panda\\": "src/Panda/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}