{
    "name": "panda/framework",
    "description": "The Panda PHP Framework.",
    "keywords": ["framework", "panda"],
    "license": "MIT",
    "homepage": "http://pandaphp.org",
    "support": {
        "issues": "https://github.com/PandaPlatform/panda-framework/issues",
        "source": "https://github.com/PandaPlatform/panda-framework"
    },
    "authors": [
        {
            "name": "Ioannis Papikas",
            "email": "papikas.ioan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "monolog/monolog": "~1.19",
        "symfony/http-foundation": "2.8.*|3.0.*",
        "symfony/http-kernel": "2.8.*|3.0.*",
        "php-di/php-di": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Panda\\": "src/Panda/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}